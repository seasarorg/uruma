<project name="uruma-std-blank" default="makeArchive" basedir=".">
	<property name="PRJ_NAME" value="uruma-std-blank" />

	<target name="makeArchive">
		<zip destfile="${RELEASE}/${PRJ_NAME}-${VER}.zip">
			<zipfileset dir="../${PRJ_NAME}">
				<exclude name="**/.svn/**" />
				<exclude name="build.xml" />
				<exclude name="log/**" />
			</zipfileset>
		</zip>
	</target>
</project>
